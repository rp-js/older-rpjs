{
  "name": "rpjs",
  "description": "A landing page for rpjs meetups!",
  "version": "1.0.0",
  "private": true,
  "author": "Jbnado (bernardojoao9@gmail.com)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript",
    "tailwind",
    "theme",
    "meetup",
    "rpjs"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "pretty-quick --staged",
    "postinstall": "husky install",
    "commit": "git-cz"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@fontsource/inter": "4.2.1",
    "@prisma/client": "4.0.0",
    "@svgr/webpack": "5.5.0",
    "axios": "0.27.2",
    "file-saver": "2.0.5",
    "html-to-image": "1.9.0",
    "next": "10.0.7",
    "next-auth": "4.9.0",
    "next-s3-upload": "0.2.2",
    "next-seo": "4.20.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "twind": "0.15.4"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/file-saver": "2.0.5",
    "@types/node": "14.14.28",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@typescript-eslint/eslint-plugin": "4.15.1",
    "@typescript-eslint/parser": "4.15.1",
    "autoprefixer": "10.4.7",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.20.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.0.9",
    "lint-staged": "10.5.4",
    "postcss": "8.4.14",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.3",
    "prisma": "4.0.0",
    "tailwindcss": "3.1.4",
    "typescript": "4.1.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
